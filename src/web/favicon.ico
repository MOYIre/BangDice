<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" viewBox="0 0 100 100">
  <!-- BanG Dream渐变背景 -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF7EB3;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8765B3;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="guitarGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFC84D;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#FF7F4D;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- 圆形背景 -->
  <circle cx="50" cy="50" r="48" fill="url(#bgGradient)" stroke="#ffffff" stroke-width="2"/>
  
  <!-- 电吉他轮廓 -->
  <path d="M25 40 C15 40, 15 60, 25 60 L45 60 L55 70 L65 60 L80 60 C90 60, 90 40, 80 40 L65 40 L55 30 L45 40 Z" 
        fill="url(#guitarGradient)" stroke="white" stroke-width="2" stroke-linejoin="round"/>
  
  <!-- 吉他拾音器 -->
  <rect x="35" y="48" width="30" height="4" rx="2" fill="#333" stroke="white" stroke-width="0.5"/>
  
  <!-- 吉他琴头 -->
  <rect x="20" y="45" width="10" height="10" rx="2" fill="#8B4513" stroke="white" stroke-width="0.5"/>
  
  <!-- 弦 -->
  <line x1="30" y1="45" x2="75" y2="45" stroke="white" stroke-width="0.8"/>
  <line x1="30" y1="50" x2="75" y2="50" stroke="white" stroke-width="0.8"/>
  <line x1="30" y1="55" x2="75" y2="55" stroke="white" stroke-width="0.8"/>
</svg>